{
  "name": "privicoin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "animate.css": "^4.1.1",
    "gsap": "^3.12.7",
    "jquery": "^3.7.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1",
    "sass": "^1.85.0"
  },
  "scripts": {
    "start": "concurrently \"npm run watch-scss\" \"react-app-rewired start\"",
    "build": "npm run build-scss && react-app-rewired build",
    "watch-scss": "concurrently \"sass --watch src/assets/sass/index.scss:src/index.css --no-source-map\" \"npx postcss src/index.css --use autoprefixer cssnano -o src/index.css --no-source-map --no-map --watch\"",
    "build-scss": "concurrently --kill-others-on-fail \"sass src/assets/sass/index.scss:src/index.css --no-source-map\" \"npx postcss src/index.css --use autoprefixer cssnano -o src/index.css\""
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cssnano": "^7.0.6",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.0",
    "postcss-loader": "^8.1.1",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^4.0.8"
  }
}
